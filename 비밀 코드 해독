from itertools import combinations

def solution(n, q, ans):
    answer = 0
    
    # 1부터 n까지의 숫자 중 5개를 뽑는 모든 조합을 생성합니다.
    # n이 최대 30이므로 전체 조합의 개수는 142,506개입니다.
    for combo in combinations(range(1, n + 1), 5):
        is_possible = True
        
        # 각 시도(q)와 시스템의 응답(ans)이 현재 조합과 일치하는지 확인합니다.
        for i in range(len(q)):
            # 현재 조합(combo)과 질문(q[i])에 공통으로 포함된 숫자의 개수를 셉니다.
            # set의 교집합(&)을 이용하면 빠르게 계산할 수 있습니다.
            match_count = len(set(combo) & set(q[i]))
            
            # 실제 일치하는 개수가 시스템 응답(ans[i])과 다르면 이 조합은 탈락입니다.
            if match_count != ans[i]:
                is_possible = False
                break
        
        # 모든 질문 조건을 만족하면 가능한 비밀 코드 조합으로 카운트합니다.
        if is_possible:
            answer += 1
            
    return answer
